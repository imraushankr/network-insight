{
  "name": "network-insight",
  "version": "1.0.0",
  "description": "Comprehensive network and client information utilities for Node.js with TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/integration",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "prepack": "npm run build"
  },
  "keywords": [
    "network",
    "ip",
    "geolocation",
    "express",
    "middleware",
    "typescript",
    "ip-lookup",
    "network-utilities",
    "client-info",
    "ip-api"
  ],
  "author": "Raushan Kumar <raushan.kumar.19u@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.3",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imraushankr/network-insight.git"
  },
  "homepage": "https://github.com/imraushankr/network-insight#readme",
  "bugs": {
    "url": "https://github.com/imraushankr/network-insight/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
